        <div class="blog-wrapper-inner">
          
        
        {.repeated section items}

        <article id="post-{id}" class="BlogList-item {@|item-classes}{.if categories.0} has-categories{.end}{.if tags.0} has-tags{.end}{.if location.addressTitle || location.addressLine1 || location.addressLine2 || location.addressCountry} has-location{.end}{.comments?} has-comments{.end} resource-item" data-item-id="{id}">
        <div class="top-aligment">
          
        
          {.main-image?}
          <div class="BlogList-item-image">
