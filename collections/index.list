  <div class="blog-pop mfp-hide" id="blog-excerpt-{id}">
    <div class="res-body">
      {excerpt|safe}
    </div>
  </div>
  {.end}
      
      <squarespace:block-field id="blog-{id}" columns="12" label="Content" class="title-section Index-page-content"/>
      <section class="Index-page-content BlogList{.equal? collection.features.systemBlogCollection.postDisplay "full"} BlogList--posts-full{.or} BlogList--posts-excerpt{.end}{.if items.0}{.or} BlogList--empty{.end} sqs-blog-list clear" data-columns="4">
