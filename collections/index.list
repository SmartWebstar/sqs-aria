        {.repeated section items}
        <div class="associates-item">
          <img src="{assetUrl}" alt="">
        </div>
        {.end}
      </div>
    </div>
    </div>
  </section>


  {.or}
  {.equal? typeName "discussions"}
  
  <section id={urlId} class="discussions-wrapper" >
    <div class="Index-page-content">
      <div class="wrap-inner-discussion">
        <div class="discussion-list">
          {.repeated section items}
          <a class="disc-item" href="{fullUrl}">
            <h3 class="disc-title">{title}</h3>
            <div class="disc-excerpt">{excerpt}</div>
          </a>
          {.end}
        </div>
      </div>
    </div>
  </section>

  {.or}

  {.equal? typeName "blog"}
