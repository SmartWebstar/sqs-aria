          </div>
          {.end}
          <time class="Blog-meta-item Blog-meta-item--date" datetime="{addedOn|date %F}">{addedOn|date %B %d, %Y}</time>

        <!--   <a href="{.passthrough?}{sourceUrl}{.or}{fullUrl}{.end}" class="BlogList-item-title"{.passthrough?} target="_blank{.end}" data-content-field="title">{title}</a> -->

          <div class="BlogList-item-title">
            {title}
          </div>

          {.equal? collection.features.systemBlogCollection.postDisplay "full"}
          <div class="BlogList-item-body">{body}</div>
          {.or}
          {.section excerpt}
          <div class="BlogList-item-excerpt more">
            {excerpt|safe|truncate 200}
            <a href="#resources-excerpt-{id}" class="open-popup-link">Read More</a>
            <!-- <a href="{.passthrough?}{sourceUrl}{.or}{fullUrl}{.end}" class="BlogList-item-readmore"></a> -->
          </div>
          {.end}
          {.end}

          <div class="Blog-meta BlogList-item-meta">
